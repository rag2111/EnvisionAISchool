{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenAIOptions": {
    "Source": "AzureOpenAI",
    "ChatModelId": "<AZUREAI_MODELID>",
    "ApiKey": "<AZUREAI_APIKEY>",
    "ChatDeploymentName": "<AZUREAI_DEPLOYMENTNAME>",
    "Endpoint": "<AZUREAI_ENDPOINT>"
  },
  "HueBridgeOptions": {
    "IpAddress": "BRIDGE_IP",
    "ApiKey": "BRIDGE_APIKEY"
  },
  "BingSearchOptions": {
    "ApiKey": "<BING_APIKEY>",
    "Endpoint": "<BING_ENDPOINT>"
  }
}
